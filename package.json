{
  "name": "vue-common-template",
  "version": "1.0.0",
  "description": "基于vue的常规资源配置。包括axios请求封装、打包环境区分等。",
  "author": "Pengo",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --config build/webpack.dev.conf.js",
    "build:test": "cross-env NODE_ENV=test node build/build.js",
    "build:test2": "cross-env NODE_ENV=test2 node build/build.js",
    "build:test3": "cross-env NODE_ENV=test3 node build/build.js",
    "build:pre": "cross-env NODE_ENV=pre node build/build.js",
    "build": "cross-env NODE_ENV=production node build/build.js",
    "clean-dll": "rimraf src/dll",
    "build:dll": "npm run clean-dll && webpack --progress --colors --config build/webpack.config.dll.js",
    "lint": "eslint --fix ./src/"
  },
  "dependencies": {
